' Module Layer
package AuthenticationModule {
    class UserAuthenticator {
        +username: string
        +password: string

        +login(username, password): boolean
        +register(username, password): boolean
        +resetPassword(username, newPassword): boolean
    }
    class N1 <<api>> "Handles user authentication logic" as N1
    UserAuthenticator ..> N1

    class SessionManager {
        +sessionId: string
        +username: string

        +createSession(username): boolean
        +destroySession(sessionId): boolean
    }
    class N2 <<api>> "Manages user session data" as N2
    SessionManager ..> N2
}

package DashboardModule {
    class UserProfile {
        +username: string
        +email: string
        +profilePicture: string

        +editProfile(profile): boolean
        +viewProfile(): boolean
    }
    class N3 <<api>> "Handles user profile management" as N3
    UserProfile ..> N3

    class HabitTracker {
        +username: string
        +habitName: string

        +trackHabit(habitName): boolean
        +viewHabitProgress(): boolean
    }
    class N4 <<api>> "Manages user habit tracking data" as N4
    HabitTracker ..> N4
}

